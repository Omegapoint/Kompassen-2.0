{
  "name": "webapp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "react-scripts build",
    "format:check": "prettier --check \"src/**/*\" \"!**/__snapshots__/**\" \"!**/.DS_Store/**\" \"!src/**/*.svg\"",
    "format:write": "prettier --write \"src/**/*\" \"!**/__snapshots__/**\" \"!**/.DS_Store/**\" \"!src/**/*.svg\"",
    "lint:check": "eslint --max-warnings 0 'src/**/*'",
    "start": "react-scripts start",
    "test": "echo 'Running tests... ðŸ§ª'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "react-app",
      "airbnb",
      "airbnb-typescript",
      "airbnb/hooks",
      "prettier",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "arrow-body-style": "warn",
      "@typescript-eslint/no-non-null-assertion": "off",
      "import/order": "warn",
      "import/prefer-default-export": "off",
      "react/jsx-props-no-spreading": "off",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off",
      "react/require-default-props": "off"
    }
  },
  "eslintIgnore": [
    "**/*.json",
    "**/*.snap",
    "**/*.svg",
    "src/setupProxy.js"
  ],
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "browserslist": "^4.16.5",
    "glob-parent": "^5.1.2",
    "immer": "^9.0.6",
    "nth-check": "^2.0.1"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.19.0",
    "@azure/msal-react": "^1.1.1",
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.2.0",
    "@mui/lab": "^5.0.0-alpha.58",
    "@mui/material": "^5.2.2",
    "@mui/x-data-grid": "^5.1.0",
    "date-fns": "^2.28.0",
    "emoji-picker-react": "^3.4.8",
    "http-proxy-middleware": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-minimal-pie-chart": "^8.2.0",
    "react-query": "^3.33.7",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2",
    "socket.io-client": "^4.4.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.0",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react-scripts": "^4.0.3",
    "typescript": "^4.5.2"
  }
}
